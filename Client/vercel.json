{
  "version": 2, 
  "builds": [
    {
      "src": "src/index.html", 
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)", 
      "dest": "/src/index.html" 
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)", 
      "destination": "https://your-backend-server.com/api/$1"
    }
  ]
}